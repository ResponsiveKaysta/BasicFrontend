    "@storybook/linter-config": "^2.5.0",
    "@storybook/react": "^5.3.19",
    "@types/storybook__addon-actions": "5.2.1",
    "@types/storybook__addon-info": "5.2.1",
    "@types/storybook__addon-knobs": "5.2.1",
    "@types/storybook__react": "5.2.1",
    "@types/styled-components": "5.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "scripts": {
    "start": "start-storybook -s ./public -p 9001 -c .storybook",
    "develop": "tsc --watch",
    "prebuild": "rimraf ./build",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --ignore \"src/**/*.test.tsx?\" --out-dir build --extensions \".ts,.tsx\" --source-maps",
    "build:storybook": "build-storybook -c config/storybook"
  }
