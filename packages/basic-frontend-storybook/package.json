{
  "name": "basic-frontend-storybook",
  "private": false,
  "version": "0.0.2-development",
  "main": "dist/index.js",
  "description": "This application demonstrates the possibilities of the responsiv IT Basic Frontend / Storybook.",
  "author": "responsive-it <dev-guru@responsive-it>",
  "license": "MIT",
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.13.1",
    "responsive-it-common": "file:../common",
    "styled-components": "^5.1.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "polished": "^3.6.5",
    "prismjs": "1.20.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.1",
    "@babel/plugin-transform-async-to-generator": "7.10.1",
    "@babel/plugin-transform-modules-commonjs": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@storybook/addon-a11y": "6.0.0-beta.20",
    "@storybook/addon-actions": "6.0.0-beta.20",
    "@storybook/addon-docs": "6.0.0-beta.20",
    "@storybook/addon-essentials": "6.0.0-beta.20",
    "@storybook/addon-info": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
