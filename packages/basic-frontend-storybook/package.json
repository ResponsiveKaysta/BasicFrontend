{
  "name": "@responsive-it/design-system",
  "version": "5.1.0",
  "description": "responsive-it design system",
  "repository": {
    "type": "git",
    "url": "https://github.com/responsive-kaysta/BasicFrontend.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kay Stuckenschmidt",
    "email": "dev-guru@responsive-it.biz"
